{% extends 'base.html.twig' %}

{% block title %}bonjour ListeController!
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">


		<h1>Liste des partitions</h1>
		<div class="row">
			{% for p in partitions %}
				<div class="col">
					<p>Nom de la partitions :
						{{ p.nom}}
						Interprete :
						{{p.interprete}}
						Chemin :{{p.chemin}}</p>
				</div>
			{% endfor %}
		</div>
	{% endblock body %}
